{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "description": "simple tic-tac-toe game",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --config webpack.config.js --mode=development --open",
    "build": "babel src -d lib",
    "prod:build": "webpack --mode=production",
    "dev:build": "webpack --mode=development --config webpack.config.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prettier": "prettier --config .prettierrc.json --write \"./src/**/*.{js,jsx}\"",
    "format": "npm run lint && npm run lint:fix && npm run prettier"
  },
  "author": "Linh",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^8.2.5",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^6.7.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "^2.8.2",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "classnames": "^2.3.1",
    "node-sass": "^5.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^4.13.1",
    "react-redux": "^8.0.5",
    "react-router": "^6.6.2",
    "react-router-dom": "^6.6.2"
  }
}
